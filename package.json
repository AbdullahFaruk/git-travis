{
    "name": "git-travis",
    "description": "Git sub command to show travis status",
    "version": "0.1.3",
    "dependencies": {
        "request": "~2.11.4",
        "ansi-color": "*",
        "which": "*",
        "nopt": "~2.0.0"
    },
    "devDependencies": {
        "yui-lint": "~0.1.1",
        "jshint": "~0.9.0",
        "vows": "*" 
    },
    "keywords": [
        "travis-ci", "travis", "git-cli", "builds"
    ],
    "main": "./lib/",
    "bin": {
        "git-travis": "./bin/git-travis.js"
    },
    "scripts": {
        "pretest": "jshint --config ./node_modules/yui-lint/jshint.json ./lib/ ./bin/"
    },
    "preferGlobal": "true",
    "bugs": { "url" : "http://github.com/davglass/git-travis/issues" },
    "licenses":[
        {
            "type" : "BSD",
            "url" : "https://github.com/davglass/git-travis/blob/master/LICENSE"
        }
    ],
    "repository": {
        "type":"git",
        "url":"http://github.com/davglass/git-travis.git"
    }
}
